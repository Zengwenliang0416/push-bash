{
  "name": "git-commit-helper",
  "displayName": "Git Commit Helper",
  "description": "A VS Code extension to help with Git commits",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "git-commit-helper.commit",
        "title": "Git: 提交更改（带表情）"
      }
    ],
    "configuration": {
      "title": "Git Commit Helper",
      "properties": {
        "gitCommit.proxy.enabled": {
          "type": "boolean",
          "default": false,
          "description": "是否启用代理"
        },
        "gitCommit.proxy.host": {
          "type": "string",
          "default": "127.0.0.1",
          "description": "代理服务器主机地址"
        },
        "gitCommit.proxy.port": {
          "type": "string",
          "default": "7890",
          "description": "代理服务器端口"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  }
}
